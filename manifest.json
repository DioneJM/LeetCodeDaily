 {
    "name": "LeetCode Daily",
    "version": "0.1",
    "description": "Show a single leetcode question everyday",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>",
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "background": {
        "scripts": ["dist/main.js"],
        "persistent": true
    },
    "page_action": {
        "default_popup": "src/popup.html",
        "default_icon": {
            "16": "images/logo16.png",
            "32": "images/logo32.png",
            "48": "images/logo48.png",
            "128": "images/logo128.png"
          }
    }
}
